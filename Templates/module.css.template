.colorpicker_rgb_b { background-image:url('<IMAGEPATH>ColorPickerControl/colorpicker_rgb_b.png'); top:112px; left:212px; }
.colorpicker_hsb_h { background-image:url('<IMAGEPATH>ColorPickerControl/colorpicker_hsb_h.png'); top:52px; left:282px; }
.colorpicker_hsb_s { background-image:url('<IMAGEPATH>ColorPickerControl/colorpicker_hsb_s.png'); top:82px; left:282px; }
.colorpicker_hsb_b { background-image:url('<IMAGEPATH>ColorPickerControl/colorpicker_hsb_b.png'); top:112px; left:282px; }
.colorpicker_submit { position:absolute; width:22px; height:22px; background:url('<IMAGEPATH>ColorPickerControl/colorpicker_submit.png') top; left:322px; top:142px; overflow:hidden; }
.colorpicker_focus { background-position:center; }
.colorpicker_hex.colorpicker_focus { background-position:bottom; }
